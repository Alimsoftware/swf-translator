{
  "name": "swf",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @kiootic/swf-translator dev",
    "build": "yarn workspaces run build",
    "docs": "spec-md docs/translator.md > docs/translator.html; spec-md docs/runtime.md > docs/runtime.html",
    "format": "yarn workspaces run format",
    "format:check": "yarn workspaces run format:check"
  },
  "devDependencies": {
    "spec-md": "1.1.1"
  }
}
