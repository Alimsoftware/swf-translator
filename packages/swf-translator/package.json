{
  "name": "@kiootic/swf-translator",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -r dist && tsc --build",
    "start": "node dist/index.js",
    "dev": "tsc --build && node dist/index.js",
    "format": "prettier --write -l 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "generate": "lezer-generator src/as3/as3.grammar -o src/as3/parser"
  },
  "dependencies": {
    "consola": "2.14.0",
    "glob": "7.1.6",
    "json5": "2.1.3",
    "lezer": "0.10.1",
    "libtess": "1.2.2",
    "mkdirp": "1.0.4",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sharp": "0.25.4",
    "ts-morph": "7.1.2",
    "typescript": "3.9.6",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/json5": "0.0.30",
    "@types/mkdirp": "1.0.1",
    "@types/node": "12.12.50",
    "@types/prettier": "2.0.2",
    "@types/rimraf": "3.0.0",
    "@types/sharp": "0.25.0",
    "@types/yargs": "15.0.5",
    "lezer-generator": "0.10.1"
  }
}
